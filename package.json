{
  "name": "cryptonodejs",
  "version": "0.0.1",
  "description": "Cryptocurrency Dashboard",
  "main": "app/index.js",
  "proxy": "http://localhost:5000",
  "scripts": {
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
